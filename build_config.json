{
  "--name": "ir_tool_UI",
  "--workpath": "build",
  "--distpath": "dist",
  "--icon": {
    "Windows": "UI/ir_tool.ico",
    "Darwin": "UI/ir_tool.icns",
    "Linux": null
  },
  "--splash": {
    "Windows": "UI/ir_tool_splash.png",
    "Darwin": null,
    "Linux": "UI/ir_tool_splash.png"
  },
  "--onefile": true,
  "--noconfirm": true,
  "--noconsole": true,
  "--add-data": [
    "*.py:.",
    "process_complete.flac:.",
    "UI:UI/"
  ],
  "--hidden-import": [
    ""
  ],
  "--exclude": [
    "tkinter",
    "matplotlib",
    "pip",
    "pyinstaller"
  ],
  "--optimize": 2,
  "--version-file": {
    "Windows": "ir_tool_UI_version.txt"
  },
  "--noupx": true
}